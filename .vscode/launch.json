  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run approvals",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/server/main.go",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Run sync",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/devcli/main.go",
      "args": ["sync"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
